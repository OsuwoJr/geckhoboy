{
  "framework": "svelte",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".svelte-kit",
  "images": {
    "sizes": [640, 828, 1200, 1920, 3840],
    "formats": ["image/avif", "image/webp"],
    "minimumCacheTTL": 300
  },
  "functions": {
    "**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
} 